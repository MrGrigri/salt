{
  "name": "@richkode/salt",
  "version": "0.0.1",
  "private": false,
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/salt/salt.esm.js",
  "exports": {
    ".": {
      "import": "./dist/salt/salt.esm.js",
      "require": "./dist/salt/salt.cjs.js"
    },
    "./salt-accordion": {
      "import": "./dist/components/salt-accordion.js",
      "types": "./dist/components/salt-accordion.d.ts"
    },
    "./salt-accordion-header": {
      "import": "./dist/components/salt-accordion-header.js",
      "types": "./dist/components/salt-accordion-header.d.ts"
    },
    "./salt-accordion-panel": {
      "import": "./dist/components/salt-accordion-panel.js",
      "types": "./dist/components/salt-accordion-panel.d.ts"
    },
    "./salt-accordion-group": {
      "import": "./dist/components/salt-accordion-group.js",
      "types": "./dist/components/salt-accordion-group.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build": "rm -rf ./dist ./loader && stencil build",
    "build:watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "start:no-open": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.8.4",
    "@stencil/core": "^4.7.0",
    "@stencil/store": "^2.0.15",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT"
}
